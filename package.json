{
  "name": "minecraft-launcher-lib",
  "version": "0.3.7",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=10.0"
  },
  "scripts": {
    "start": "node ./lib/examples/launcher",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "concurrently -n types,js \"npm run build:types\" \"npm run build:js\"",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps",
    "test": "jest \"lib/.*\\.test\\.js\" --env=node",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maksimetny/minecraft-launcher-lib.git"
  },
  "keywords": [
    "minecraft",
    "lib",
    "launcher",
    "node",
    "minecraft-launcher-lib"
  ],
  "author": {
    "email": "maksimetny@gmail.com",
    "url": "https://maksimetny.com",
    "name": "Dmitry Maksimetny"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maksimetny/minecraft-launcher-lib/issues"
  },
  "homepage": "https://github.com/maksimetny/minecraft-launcher-lib#readme",
  "dependencies": {
    "axios": "^0.22.0",
    "mkdirp": "^1.0.4",
    "uuid": "^8.3.2",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-typescript": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.2",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.11.0",
    "@types/uuid": "^8.3.1",
    "@types/yauzl": "^2.9.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.2.5",
    "nock": "^13.1.3",
    "typescript": "^4.4.4"
  }
}
